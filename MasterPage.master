    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <link rel="icon" type="image/x-icon" href="/Images/piccol.ico" />
</head>
<body>

    <form id="form1" runat="server">
        <asp:ScriptManager runat="server" ID="ScriptManager"></asp:ScriptManager>

            <section class='pt-1 Mario d-flex align-items-center justify-content-center'>
        <p class='mb-1 sconti'>Sconti fino al 40% su Mario.</p>
    </section>
    <nav class="navbar navbar-expand-lg p-1 ">
        <div class="container-fluid m-0">
            <a class="navbar-brand ms-5 me-1 p-0" href="Home.aspx">
                <img src="data:image/jpeg;base64," alt="Logo" class="logo">
            </a>
            <button class="navbar-toggler me-4" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse d-flex align-items-center" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="Home.aspx">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="Carrello.aspx">Carrello</a>
                    </li>
                </ul>
                <asp:Label ID="lblCartCount" runat="server" CssClass="cart-count"></asp:Label>
                <asp:LoginView ID="LoginView1" runat="server">
                    <AnonymousTemplate>
                        <ul class="mb-0 me-5">
                            <li class="nav-item">
                                <asp:Button runat="server" ID="btnLogin" Text="Login" OnClick="btnLogin_Click" CssClass="btn btn-primary" />
                            </li>
                        </ul>
                    </AnonymousTemplate>
                    <LoggedInTemplate>
                        <ul class="m-0">
                            <li class="nav-item">
                                <asp:Button runat="server" ID="btnLogout" Text="Logout" OnClick="btnLogout_Click" CssClass="btn btn-danger" />
                            </li>
                        </ul>
                    </LoggedInTemplate>
                </asp:LoginView>

            </div>
        </div>
    </nav>
        <div>
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">

            </asp:ContentPlaceHolder>
        </div>
    </form>
    <footer class="w-100 p-3 text-white">
